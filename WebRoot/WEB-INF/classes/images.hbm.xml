<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
		
<hibernate-mapping package="model">
	<class name="cos.model.bean.OfficalPostImageBean" table="offical_post_image">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="image_name"></property>
		<property name="postid"></property>
	</class>
	
	<class name="cos.model.bean.OfficalPhotoPathBean" table="offical_photo_path">
		<id name="id" column="id">
		</id>
		<property name="path_file"></property>
	</class>
</hibernate-mapping>